{
	"name": "ViewLater+",
	"manifest_version": 3,
	"version": "1.0.0",
	"description": "Save and organize your favorite videos with notes. Easily keep track of what to watch next!",
	"icons": {
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": ["storage"],
	"background": {
    "service_worker": "background.js",
	"type": "module"
	},
	"action": {
		"default_title": "ViewLater+",
		"default_popup": "popup.html"
	},
	"options_page": "options.html"
	
}